			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>
		<action name="welcomeIrro" class="com.saasforedu.irro.action.ExampleSupport">
			<result name="success" type="tiles">welcome</result>
		</action>

